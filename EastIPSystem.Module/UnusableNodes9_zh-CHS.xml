<?xml version="1.0" encoding="utf-8"?>
<Application>
  <Views Id="Views">
    <DetailView Id="Notification_DetailView">
      <Layout Id="Layout">
        <LayoutGroup Id="Main">
          <LayoutGroup Id="SimpleEditors">
            <LayoutGroup Id="Notification" Caption="通知" />
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="NotificationsObject_DetailView">
      <Items Id="Items">
        <PropertyEditor Id="Postpone" Caption="暂停时间" />
      </Items>
    </DetailView>
    <DetailView Id="PostponeTime_DetailView">
      <Items Id="Items">
        <PropertyEditor Id="RemindIn" Caption="提醒间隔" />
      </Items>
      <Layout Id="Layout">
        <LayoutGroup Id="Main">
          <LayoutGroup Id="SimpleEditors">
            <LayoutGroup Id="PostponeTime" Caption="推迟时间" />
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <ListView Id="PostponeTime_ListView">
      <Columns Id="Columns">
        <ColumnInfo Id="RemindIn" Caption="提醒间隔" />
      </Columns>
    </ListView>
    <ListView Id="PostponeTime_LookupListView">
      <Columns Id="Columns">
        <ColumnInfo Id="RemindInText" Caption="提醒间隔" />
      </Columns>
    </ListView>
  </Views>
  <BOModel Id="BOModel">
    <Class Name="DevExpress.ExpressApp.Notifications.CommonNotificationItem">
      <OwnMembers Id="OwnMembers">
        <Member Name="NotificationSource" Caption="通知源" />
      </OwnMembers>
    </Class>
    <Class Name="DevExpress.ExpressApp.Notifications.Notification" Caption="通知">
      <OwnMembers Id="OwnMembers">
        <Member Name="AlarmTime" Caption="提醒时间" />
        <Member Name="Subject" Caption="标题" />
      </OwnMembers>
    </Class>
    <Class Name="DevExpress.ExpressApp.Notifications.NotificationsObject" ObjectCaptionFormat="{0:NotificationsCount} 条通知">
      <OwnMembers Id="OwnMembers">
        <Member Name="Notifications" Caption="通知" />
        <Member Name="NotificationsState" Caption="通知状态" />
        <Member Name="Postpone" Caption="推迟" />
        <Member Name="ShowNotificationsWindow" Caption="显示通知窗口" />
      </OwnMembers>
    </Class>
    <Class Name="DevExpress.ExpressApp.SystemModule.Notifications.PostponeTime" Caption="推迟时间">
      <OwnMembers Id="OwnMembers">
        <Member Name="RemindIn" Caption="提醒间隔" />
        <Member Name="RemindInText" Caption="提醒间隔" />
      </OwnMembers>
    </Class>
  </BOModel>
  <Localization Id="Localization">
    <LocalizationGroup Name="Notifications">
      <LocalizationItem Name="NotificationViewCaption" Value="通知" />
      <LocalizationItem Name="ShowNotificationsActionEnabledTooltip" Value="显示通知" />
    </LocalizationGroup>
    <LocalizationGroup Name="Enums">
      <LocalizationGroup Name="DevExpress.ExpressApp.Notifications.NotificationItemState" Value="通知项目状态">
        <LocalizationItem Name="Active" Value="有效的" />
        <LocalizationItem Name="Postponed" Value="已推迟" />
      </LocalizationGroup>
      <LocalizationGroup Name="DevExpress.ExpressApp.Notifications.NotificationsType" Value="通知类型">
        <LocalizationItem Name="Active" Value="活动的" />
        <LocalizationItem Name="All" Value="全部" />
        <LocalizationItem Name="Postponed" Value="已推迟" />
      </LocalizationGroup>
    </LocalizationGroup>
    <LocalizationGroup Name="Namespaces">
      <LocalizationItem Name="DevExpress.ExpressApp.Notifications" Value="通知" />
      <LocalizationItem Name="DevExpress.ExpressApp.SystemModule.Notifications" Value="通知" />
    </LocalizationGroup>
  </Localization>
  <ActionDesign Id="ActionDesign">
    <Actions Id="Actions">
      <Action Id="Dismiss" Caption="消除" />
      <Action Id="DismissAll" Caption="全部消除" />
      <Action Id="Snooze" Caption="暂停" />
    </Actions>
  </ActionDesign>
</Application>